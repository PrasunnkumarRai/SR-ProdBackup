<?xml version="1.0" encoding="UTF-8"?>
<ApprovalProcess xmlns="http://soap.sforce.com/2006/04/metadata">
    <active>true</active>
    <allowRecall>false</allowRecall>
    <allowedSubmitters>
        <type>creator</type>
    </allowedSubmitters>
    <allowedSubmitters>
        <type>owner</type>
    </allowedSubmitters>
    <allowedSubmitters>
        <submitter>President_Media</submitter>
        <type>roleSubordinates</type>
    </allowedSubmitters>
    <approvalPageFields>
        <field>Name</field>
        <field>Owner</field>
    </approvalPageFields>
    <approvalStep>
        <allowDelegate>false</allowDelegate>
        <assignedApprover>
            <approver>
                <name>Digital_Expedite_East</name>
                <type>queue</type>
            </approver>
            <whenMultipleApprovers>FirstResponse</whenMultipleApprovers>
        </assignedApprover>
        <entryCriteria>
            <criteriaItems>
                <field>Digital_Creative__c.Region__c</field>
                <operation>equals</operation>
                <value>Southeast,Northern,Blue Ridge</value>
            </criteriaItems>
        </entryCriteria>
        <ifCriteriaNotMet>GotoNextStep</ifCriteriaNotMet>
        <label>Priority Expedite - East</label>
        <name>Priority_Expedite_East</name>
    </approvalStep>
    <approvalStep>
        <allowDelegate>false</allowDelegate>
        <assignedApprover>
            <approver>
                <name>Digital_Expedite_West</name>
                <type>queue</type>
            </approver>
            <whenMultipleApprovers>FirstResponse</whenMultipleApprovers>
        </assignedApprover>
        <entryCriteria>
            <criteriaItems>
                <field>Digital_Creative__c.Region__c</field>
                <operation>equals</operation>
                <value>West,Adlink / SRS</value>
            </criteriaItems>
        </entryCriteria>
        <ifCriteriaNotMet>GotoNextStep</ifCriteriaNotMet>
        <label>Priority Expedite - West</label>
        <name>Priority_Expedite_West</name>
        <rejectBehavior>
            <type>RejectRequest</type>
        </rejectBehavior>
    </approvalStep>
    <approvalStep>
        <allowDelegate>false</allowDelegate>
        <assignedApprover>
            <approver>
                <name>Digital_Expedite_Central</name>
                <type>queue</type>
            </approver>
            <whenMultipleApprovers>FirstResponse</whenMultipleApprovers>
        </assignedApprover>
        <entryCriteria>
            <criteriaItems>
                <field>Digital_Creative__c.Region__c</field>
                <operation>equals</operation>
                <value>Central,Great Lakes</value>
            </criteriaItems>
        </entryCriteria>
        <label>Priority Expedite - Central</label>
        <name>Priority_Expedite_Central</name>
        <rejectBehavior>
            <type>RejectRequest</type>
        </rejectBehavior>
    </approvalStep>
    <emailTemplate>unfiled$public/DCC_Expedite_DCC_Approval_Submitted_Notification</emailTemplate>
    <enableMobileDeviceAccess>false</enableMobileDeviceAccess>
    <entryCriteria>
        <formula>AND(
ISPICKVAL(Status__c, &apos;Submit for Expedite Approval&apos;),
NOT(ISPICKVAL(Is_Expedite_Approved__c, &apos;TRUE&apos;))
)</formula>
    </entryCriteria>
    <finalApprovalActions>
        <action>
            <name>DCC_Expedite_Approved_Status_Update</name>
            <type>FieldUpdate</type>
        </action>
        <action>
            <name>DCC_Expedite_DC_Approved_Notification</name>
            <type>Alert</type>
        </action>
        <action>
            <name>DCC_Is_Expedite_Approved_Update_TRUE</name>
            <type>FieldUpdate</type>
        </action>
    </finalApprovalActions>
    <finalApprovalRecordLock>false</finalApprovalRecordLock>
    <finalRejectionActions>
        <action>
            <name>DCC_Expedite_DC_Rejected_Notification</name>
            <type>Alert</type>
        </action>
        <action>
            <name>DCC_Expedite_Rejected_Status_Update</name>
            <type>FieldUpdate</type>
        </action>
        <action>
            <name>DCC_Is_Expedite_Approved_Update_FALSE</name>
            <type>FieldUpdate</type>
        </action>
    </finalRejectionActions>
    <finalRejectionRecordLock>false</finalRejectionRecordLock>
    <label>DCC | Expedite Approval</label>
    <nextAutomatedApprover>
        <useApproverFieldOfRecordOwner>false</useApproverFieldOfRecordOwner>
        <userHierarchyField>Approving_Manager__c</userHierarchyField>
    </nextAutomatedApprover>
    <processOrder>1</processOrder>
    <recallActions>
        <action>
            <name>DCC_Expedite_Recalled_Status_Update</name>
            <type>FieldUpdate</type>
        </action>
    </recallActions>
    <recordEditability>AdminOrCurrentApprover</recordEditability>
    <showApprovalHistory>false</showApprovalHistory>
</ApprovalProcess>
