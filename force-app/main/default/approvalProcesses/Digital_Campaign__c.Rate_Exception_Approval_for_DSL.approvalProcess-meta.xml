<?xml version="1.0" encoding="UTF-8"?>
<ApprovalProcess xmlns="http://soap.sforce.com/2006/04/metadata">
    <active>false</active>
    <allowRecall>false</allowRecall>
    <allowedSubmitters>
        <type>creator</type>
    </allowedSubmitters>
    <allowedSubmitters>
        <type>owner</type>
    </allowedSubmitters>
    <allowedSubmitters>
        <submitter>President_Media</submitter>
        <type>roleSubordinates</type>
    </allowedSubmitters>
    <approvalPageFields>
        <field>Name</field>
        <field>Owner</field>
        <field>CPM_Negotiat__c</field>
    </approvalPageFields>
    <approvalStep>
        <allowDelegate>false</allowDelegate>
        <assignedApprover>
            <approver>
                <name>East_DSL</name>
                <type>queue</type>
            </approver>
            <whenMultipleApprovers>FirstResponse</whenMultipleApprovers>
        </assignedApprover>
        <entryCriteria>
            <criteriaItems>
                <field>Digital_Campaign__c.Region__c</field>
                <operation>equals</operation>
                <value>Southeast,Northern,Blue Ridge</value>
            </criteriaItems>
        </entryCriteria>
        <ifCriteriaNotMet>GotoNextStep</ifCriteriaNotMet>
        <label>Approval for East DSL</label>
        <name>Approval_for_East_DSL</name>
    </approvalStep>
    <approvalStep>
        <allowDelegate>false</allowDelegate>
        <assignedApprover>
            <approver>
                <name>West_DSL</name>
                <type>queue</type>
            </approver>
            <whenMultipleApprovers>FirstResponse</whenMultipleApprovers>
        </assignedApprover>
        <entryCriteria>
            <criteriaItems>
                <field>Digital_Campaign__c.Region__c</field>
                <operation>equals</operation>
                <value>West,Adlink / SRS</value>
            </criteriaItems>
        </entryCriteria>
        <ifCriteriaNotMet>GotoNextStep</ifCriteriaNotMet>
        <label>Approval for West DSL</label>
        <name>Approval_for_West_DSL</name>
        <rejectBehavior>
            <type>RejectRequest</type>
        </rejectBehavior>
    </approvalStep>
    <approvalStep>
        <allowDelegate>false</allowDelegate>
        <assignedApprover>
            <approver>
                <name>Central_DSL</name>
                <type>queue</type>
            </approver>
            <whenMultipleApprovers>FirstResponse</whenMultipleApprovers>
        </assignedApprover>
        <entryCriteria>
            <criteriaItems>
                <field>Digital_Campaign__c.Region__c</field>
                <operation>equals</operation>
                <value>Central,Great Lakes</value>
            </criteriaItems>
        </entryCriteria>
        <label>Approval for Central DSL</label>
        <name>Approval_for_Central_DSL</name>
        <rejectBehavior>
            <type>RejectRequest</type>
        </rejectBehavior>
    </approvalStep>
    <emailTemplate>unfiled$public/DC_Rate_Exception_Approval_Submitted_Notification</emailTemplate>
    <enableMobileDeviceAccess>false</enableMobileDeviceAccess>
    <entryCriteria>
        <formula>AND(
NOT(ISNULL(CPM_Negotiat__c)),
NOT(ISPICKVAL(Division__c , &apos;National&apos;)),
ISPICKVAL(Status__c, &quot;Draft Plan&quot;)
)</formula>
    </entryCriteria>
    <finalApprovalActions>
        <action>
            <name>Approved_DSL</name>
            <type>FieldUpdate</type>
        </action>
        <action>
            <name>DC_Rate_Exception_Approved_Notification</name>
            <type>Alert</type>
        </action>
        <action>
            <name>Status_Plan_Submitted</name>
            <type>FieldUpdate</type>
        </action>
    </finalApprovalActions>
    <finalApprovalRecordLock>false</finalApprovalRecordLock>
    <finalRejectionActions>
        <action>
            <name>DC_CPM_IR_Reject_DC_Status_Update</name>
            <type>FieldUpdate</type>
        </action>
        <action>
            <name>DC_Rate_Exception_Rejected_Notification</name>
            <type>Alert</type>
        </action>
        <action>
            <name>Rejected_DSL</name>
            <type>FieldUpdate</type>
        </action>
    </finalRejectionActions>
    <finalRejectionRecordLock>false</finalRejectionRecordLock>
    <initialSubmissionActions>
        <action>
            <name>DSL_Submission</name>
            <type>FieldUpdate</type>
        </action>
    </initialSubmissionActions>
    <label>Rate Exception Approval for DSL</label>
    <processOrder>1</processOrder>
    <recordEditability>AdminOnly</recordEditability>
    <showApprovalHistory>false</showApprovalHistory>
</ApprovalProcess>
