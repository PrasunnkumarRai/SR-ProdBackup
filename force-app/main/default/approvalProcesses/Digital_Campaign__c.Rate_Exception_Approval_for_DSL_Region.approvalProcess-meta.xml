    <emailTemplate>unfiled$public/DC_Rate_Exception_Approval_Submitted_Notification</emailTemplate>
    <enableMobileDeviceAccess>false</enableMobileDeviceAccess>
    <entryCriteria>
        <formula>AND(
NOT(ISNULL(CPM_Negotiat__c)),
ISPICKVAL(Status__c, &quot;Draft Plan&quot;)
)</formula>
    </entryCriteria>
    <finalApprovalActions>
        <action>
            <name>Approved_DSL</name>
