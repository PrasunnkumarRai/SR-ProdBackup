                <name>Adlink_AA_Approvers</name>
                <type>queue</type>
            </approver>
            <whenMultipleApprovers>FirstResponse</whenMultipleApprovers>
        </assignedApprover>
        <entryCriteria>
            <criteriaItems>
                <field>Digital_Campaign__c.Region__c</field>
                <operation>equals</operation>
